		<style type="text/css">
html,body {
	font: normal 12px verdana;
	margin: 0;
	padding: 0;
	border: 0 none;
	overflow: hidden;
	height: 100%;
}

.empty .x-panel-body {
	padding-top: 0;
	text-align: center;
	font-style: italic;
	color: gray;
	font-size: 11px;
}

.x-btn button {
	font-size: 14px;
}

.x-panel-header {
	font-size: 14px;
}
</style>
<script type="text/javascript">
	Ext.onReady( function() {
		//Ext.Msg.alert('ext','welcome you!');
		var addPanel = function(btn, event) {
			var n;
			n = tabPanel.getComponent(btn.id);
			if(n) {
				tabPanel.setActiveTab(n);
				return;
			}
			n = tabPanel.add( {
				id : btn.id,
				title : btn.id,
				html : '<iframe width=100% height=100% src=' + btn.url + ' />',
				closable : 'true'
			});
			tabPanel.setActiveTab(n);
		}

		var item1 = new Ext.Panel( {
			title : '试验公告',
			cls : 'empty',
			items : [ 
				new Ext.Button({
					id : '试验公告',
					text : '试验公告',
					url : 'gonggao/gonggao.jsp',
					width : '100%',
					listeners : {
						click : addPanel
					}

				}),
				new Ext.Button({
					id : '试验管理',
					text : '试验管理',
					url : 'guanli/manage.jsp',
					width : '100%',
					listeners : {
						click : addPanel
					}

				})
				]
		});

		var item2 = new Ext.Panel( {
			title : '试验监测',
			cls : 'empty',
			items : [ 
				new Ext.Button({
					id : '数据监测',
					text : '数据监测',
					url : 'jiance/datashow_test.jsp',
					width : '100%',
					listeners : {
						click : addPanel
					}

				})
				]
		});

		var item3 = new Ext.Panel( {
			title : '统计分析',
			cls : 'empty',
			items : [ 
				new Ext.Button({
					id : '历史数据',
					text : '历史数据',
					url : 'tongjifenxi/statistics_showData.jsp',
					width : '100%',
					listeners : {
						click : addPanel
					}

				}),
				new Ext.Button({
					id : '图表显示',
					text : '图表显示',
					url : 'tongjifenxi/statistics_chart.jsp',
					width : '100%',
					listeners : {
						click : addPanel
					}

				}),
				new Ext.Button({
					id : '历史轨迹',
					text : '历史轨迹',
					url : 'tongjifenxi/statistics_gis.jsp',
					width : '100%',
					listeners : {
						click : addPanel
					}

				}),
				new Ext.Button({
					id : '报警情况',
					text : '报警情况',
					url : 'tongjifenxi/statistics_warning.jsp',
					width : '100%',
					listeners : {
						click : addPanel
					}

				}),
				new Ext.Button({
					id : '里程统计',
					text : '里程统计',
					url : 'tongjifenxi/statistics_mile.jsp',
					width : '100%',
					listeners : {
						click : addPanel
					}

				}),
				new Ext.Button({
					id : '数据修复',
					text : '数据修复',
					url : 'tongjifenxi/statistics_recorver.jsp',
					width : '100%',
					listeners : {
						click : addPanel
					}

				}),
				new Ext.Button({
					id : '报表生成',
					text : '报表生成',
					url : 'tongjifenxi/statistics_export.jsp',
					width : '100%',
					listeners : {
						click : addPanel
					}

				})
				]
		});

		var item4 = new Ext.Panel( {
			title : '用户管理',
			cls : 'empty'
		});

		var accordion = new Ext.Panel( {
			region : 'west',
			margins : '5 0 5 5',
			split : true,
			width : 210,
			layout : 'accordion',
			items : [ item1, item2, item3, item4 ]
		});

		var tabPanel = new Ext.TabPanel( {
			region : 'center',
			enableTabScroll : true,
			deferredRender : false,
			activeTab : 0,
			items : [ {

				title : 'index',
				html : '我的桌面'
		//		autoLoad : "gonggao/gonggao.jsp"
			} ]
		});

		var viewport = new Ext.Viewport( {
			layout : 'border',
			items : [ accordion, tabPanel ]
		});

	});
</script>
